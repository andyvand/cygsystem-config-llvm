msgid "%s megabytes"
msgstr "%s ಮೆಗಾ ಬೈಟುಗಳು"

#: src/InputController.py:134
#, python-format
msgid "%s kilobytes"
msgstr "%s ಕಿಲೋ ಬೈಟುಗಳು"

#: src/InputController.py:135
#, python-format
msgid "%s gigabytes"
msgstr "%s ಗಿಗಾ ಬೈಟುಗಳು"

#: src/InputController.py:136
#, python-format
